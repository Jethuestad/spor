{
  "name": "@vygruppen/spor-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "add-package": "plop --plopfile internal/plopfile.mjs",
    "prebuild": "rimraf packages/*/dist",
    "prebuild:web": "rimraf packages/*/dist",
    "build": "turbo run build",
    "build:rn": "turbo run build --filter=@vygruppen/spor*-react-native",
    "build:web": "turbo run build --filter=@vygruppen/spor*-react",
    "test": "turbo run test --parallel",
    "dev": "npm run dev:web",
    "dev:web": "turbo run dev --no-cache --parallel --filter=@vygruppen/spor*-react --filter=@vygruppen/docs --filter=@vygruppen/studio",
    "dev:rn": "turbo run dev --no-cache --parallel --filter=@vygruppen/spor*-react-native --filter=@vygruppen/rn-demo",
    "postinstall": "patch-package",
    "prerelease": "npm run build",
    "release": "changeset publish"
  },
  "dependencies": {
    "@changesets/cli": "^2.19.0",
    "patch-package": "^6.4.7",
    "plop": "^3.0.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "turbo": "^1.3.1"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.1.2",
  "devDependencies": {
    "@types/react-native": "^0.69.4"
  }
}
